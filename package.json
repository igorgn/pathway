{
  "name": "pathway",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "watch-test": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "detox-android": "detox build --configuration android.debug && detox test --configuration android.debug",
    "detox-ios": "detox build --configuration ios.debug && detox test --configuration ios.debug",
    "server": "nodemon server/server.ts"
  },
  "dependencies": {
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/datetimepicker": "^3.4.6",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-native-picker/picker": "^1.15.0",
    "@reduxjs/toolkit": "^1.5.1",
    "axios": "^0.21.1",
    "date-fns": "^2.21.1",
    "lodash": "^4.17.21",
    "react": "17.0.1",
    "react-component-driver": "^0.10.0",
    "react-native": "0.64.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-navigation": "^7.13.0",
    "react-native-navigation-hooks": "^6.3.0",
    "react-native-reanimated": "^2.1.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-ui-lib": "^5.22.2",
    "react-redux": "^7.2.3",
    "react-test-renderer-utils": "^2.1.0",
    "redux-component-driver": "^0.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.0",
    "@types/node-fetch": "^2.5.10",
    "@types/react-native": "^0.64.0",
    "@types/react-test-renderer": "^16.9.2",
    "@types/superagent": "^4.1.10",
    "@types/supertest": "^2.0.11",
    "babel-jest": "^26.6.3",
    "detox": "^18.12.0",
    "eslint": "^7.14.0",
    "eslint-plugin-detox": "^1.0.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "node-fetch": "^2.6.1",
    "react-test-renderer": "17.0.1",
    "superagent": "^6.1.0",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ],
    "setupFiles": [
      "./jest-setup.js"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "server/data/*"
    ]
  }
}
