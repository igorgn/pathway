{
  "name": "one-app-module-habits",
  "version": "0.0.1",
  "private": true,
  "main": "./src/module.js",
  "scripts": {
    "android": "one-app-engine --mock-mode quickLogin -a —force-localhost",
    "ios": "one-app-engine --mock-mode quickLogin -i —force-localhost",
    "start": "one-app-engine --mock-mode quickLogin -ia —force-localhost",
    "test": "jest",
    "watch-test": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "detox-android": "detox build --configuration android.debug && detox test --configuration android.debug",
    "detox-ios": "detox build --configuration ios.debug && detox test --configuration ios.debug",
    "server": "nodemon server/server.ts",
    "release": "mltci release",
    "customPublish": "mltci release"
  },
  "postinstall": "one-app-engine --setup",
  "dependencies": {
    "@reduxjs/toolkit": "1.5.1",
    "axios": "0.21.1",
    "date-fns": "2.21.1",
    "react-component-driver": "0.10.0",
    "react-redux": "7.2.3",
    "wix-mobile-lifecycle-tools": "2.309.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.0",
    "@types/node-fetch": "^2.5.10",
    "@types/react-native": "^0.64.0",
    "@types/react-test-renderer": "^16.9.2",
    "@types/superagent": "^4.1.10",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "babel-jest": "^26.6.3",
    "detox": "^18.12.0",
    "eslint": "^7.14.0",
    "eslint-plugin-detox": "^1.0.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "node-fetch": "^2.6.1",
    "react-test-renderer": "17.0.1",
    "react-test-renderer-utils": "^2.1.0",
    "redux-component-driver": "^0.10.2",
    "superagent": "^6.1.0",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^3.8.3",
    "uuid": "^8.3.2",
    "wix-one-app-engine": "ga",
    "@wix/wix-one-app-platform": "ga",
    "wix-owners-app-platform": "ga",
    "wix-one-app-member-view": "ga",
    "wix-react-native-ui-lib": "ga",
    "wix-react-native-i18n": "5.156.0"
  },
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/"
  },
  "peerDependencies": {
    "wix-one-app-engine": "*"
  },
  "oneAppEngine": {
    "admin": {
      "platformModule": "wix-owners-app-platform",
      "modules": [
        "one-app-module-habits",
        "demo-tab-module",
        "wix-owners-app-platform",
        "@wix/wix-one-app-platform",
        "wix-one-app-member-view"
      ],
      "tabs": [
        "dashboard",
        "demoTab",
        "plus"
      ]
    },
    "places": {
      "platformModule": "@wix/wix-one-app-platform",
      "modules": [
        "one-app-module-habits",
        "@wix/wix-one-app-platform",
        "demo-tab-module",
        "wix-one-app-member-view"
      ],
      "tabs": [
        "demoTab"
      ]
    }
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ],
    "setupFiles": [
      "./jest-setup.js"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "server/data/*"
    ]
  }
}
